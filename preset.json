{
  "useConfigFiles": true,
  "packageManager": "npm",
  "useTaobaoRegistry": true,
  "cssPreprocessor": "less",
  "router": true,
  "vuex": true,
  "routerHistoryMode": false,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-router": {
      "historyMode": false
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": [
        "save"
      ]
    }
  }
}